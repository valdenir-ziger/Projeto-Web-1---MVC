<h2>Ranking Top 3</h2>
<div class="table-responsive"></div>
    <table class="table table-sm bg-dark text-white text-start col-6">
        <thead class="thead-dark">
            <tr>
                <th scope="col">Evento</th>
                <th scope="col">Apresentação</th>
                <th scope="col">Nota</th>
                <th scope="col">Votos</th>
            </tr>
        </thead>
        <tbody>
            {{#each ranking}}
            <tr>
                <th scope="row">{{descricao_evento}}</th>
                <td>{{descricao_apresentacao}}</td>
                <td>{{nota}}</td>
                <td>1</td>
            {{/each}}
        </tbody>
    </table>
    <br>
    <br>
</div>
