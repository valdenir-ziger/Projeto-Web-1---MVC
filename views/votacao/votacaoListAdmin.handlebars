<h2>Lista de Votos</h2>
<div class="table-responsive"></div>
    <table class="table table-sm bg-dark text-white text-start col-6">
        <thead class="thead-dark">
            <tr>
                <th scope="col">Evento</th>
                <th scope="col">Apresentação</th>
                <th scope="col">Matrícula</th>
                <th scope="col">Nota</th>
            </tr>
        </thead>
        <tbody>
        {{#each votacao}}
        <tr>
            <th scope="row">{{descricao_evento}}</th>
            <td>{{descricao_apresentacao}}</td>
            <td>{{matricula}}</td>
            <td>{{nota}}</td>
            <td><a href="votacaoEdit/{{_id}}"> <button class="btn btn-secondary btn-sm">Editar</button> </a></td>
            {{#if excluido}}
                <td><a href="votacaoDelete/{{_id}}"> <button class="btn btn-outline-danger btn-sm" disabled>Desativado</button> </a></td>
            {{else}}
                <td><a href="votacaoDelete/{{_id}}"> <button class="btn btn-secondary btn-sm">Deletar</button> </a></td>
            {{/if}}
        {{/each}}
        </tbody>
    </table>
    <br>
    <br>
</div>
